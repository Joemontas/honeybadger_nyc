{{#each tracks}}
    <div class="album-container" style="background-image: url('{{albumArtUrl}}')">
        <h1>{{artistName}}</h1>
        <h2 class="track-name" data-id="{{trackId}}">{{trackName}}</h2>
        <h3>{{albumName}}</h3>
        <button class="play-button">Click me to Play</button>
    </div>
{{/each}}
